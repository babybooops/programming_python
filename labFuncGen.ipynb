   "source": [
    "def my_cycle(iterable):\n",
    "    while True:\n",
    "        for el in iterable:\n",
    "            yield el\n",
    "\n",
    "n = 0 \n",
    "for i in my_cycle(\"ABC\"):\n",
    "    print(i, end=' ')\n",
    "    if n > 10:\n",
    "        break\n",
    "    n += 1"
   ]
  }
